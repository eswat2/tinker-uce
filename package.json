{
  "name": "tinker-uce",
  "version": "0.0.477",
  "description": "secret...",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "cc": "prettier --check './src/**/*.{css,pcss,js}'",
    "format": "prettier --write './src/**/*.{css,pcss,js}'",
    "spell": "cspell './src/**/*.{html,js}'",
    "build": "pnpm twx; pnpm format; tsc && vite build",
    "preview": "vite preview",
    "open": "open https://tinker-uce.vercel.app"
  },
  "dependencies": {
    "between.js": "0.1.2-fix.2",
    "easing-functions": "1.3.0",
    "nanostores": "1.1.0",
    "uce": "1.18.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "cspell": "9.6.3",
    "only-allow": "1.2.2",
    "pettier": "1.1.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.468",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "yoctocolors": "2.1.2"
  }
}
