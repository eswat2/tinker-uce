{
  "name": "tinker-uce",
  "version": "0.0.407",
  "description": "secret...",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "cc": "prettier --check './src/**/*.{css,pcss,js}'",
    "format": "prettier --write './src/**/*.{css,pcss,js}'",
    "spell": "cspell './src/**/*.{html,js}'",
    "build": "yarn twx; yarn format; tsc && vite build",
    "preview": "vite preview",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.vercel; vc --prod",
    "open": "open https://tinker-uce.vercel.app"
  },
  "dependencies": {
    "between.js": "0.1.2-fix.2",
    "easing-functions": "1.3.0",
    "nanostores": "1.0.1",
    "uce": "1.18.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.0",
    "cspell": "9.2.0",
    "pettier": "1.1.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "proto-tailwindcss-clrs": "0.0.431",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "vite": "7.0.6"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
